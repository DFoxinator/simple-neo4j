{
  "name": "dfoxinator/simple-neo4j",
  "description": "Simple Neo4j Client",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "laudis/neo4j-php-client": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5",
    "vimeo/psalm": "^5.15",
    "psalm/plugin-phpunit": "^0.18.4"
  },
  "autoload": {
    "psr-4": {
      "SimpleNeo4j\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SimpleNeo4j\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
